  { "start": 125.0, "end": 126.0, "text": "Pipi papa pipi papi pap" },
  { "start": 127.0, "end": 127.0, "text": "What's the Chameleon say?, ancient mystery." },
  { "start": 132.0, "end": 138.0, "text": "Somewhere deep in the jungle, I know you're hiding." },
  { "start": 138.0, "end": 142.0, "text": "What is your sound? Will we ever know?" },
  { "start": 142.0, "end": 150.0, "text": "It will always be a mystery — what do you say?" },
