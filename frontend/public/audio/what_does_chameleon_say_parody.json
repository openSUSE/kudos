  { "start": 25.0, "end": 27.0, "text": "Piggies oink" },
  { "start": 27.0, "end": 28.0, "text": "Chips go aah aaah a" },
  { "start": 31.0, "end": 35.0, "text": "But there's one sound that no one knows" },
  { "start": 35.0, "end": 37.0, "text": "What's the Chameleon say?" },
