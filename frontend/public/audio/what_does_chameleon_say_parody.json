  { "start": 42.0, "end": 43.0, "text": "Ring ding ding ding dinga dingy ding" },
  { "start": 43.0, "end": 45.0, "text": "What's the Chameleon say?" },
  { "start": 45.0, "end": 46.0, "text": "Chachy Chachy Chachy Chow" },
  { "start": 47.0, "end": 48.0, "text": "Chachy Chachy Chachy Chow" },
  { "start": 49.0, "end": 50.0, "text": "Chachy Chachy Chachy Chow" },
  { "start": 51.0, "end": 52.0, "text": "What's the Chameleon say?" },
  { "start": 52.0, "end": 56.0, "text": "Hips dance, hips hips hips dance," },
  { "start": 56.0, "end": 58.0, "text": "hips hips hips dance" },
  { "start": 58.0, "end": 60.0, "text": "What's the Chameleon say?" },
  { "start": 61.0, "end": 62.0, "text": "Pipi papa pipi papi pap" },
  { "start": 62.0, "end": 63.0, "text": "Pipi papa pipi papi pap" },
  { "start": 63.0, "end": 64.0, "text": "Pipi papa pipi papi pap" },
  { "start": 68.0, "end": 70.0, "text": "Big green eyes, scaly nose," },
  { "start": 70.0, "end": 75.0, "text": "eating bugs and climbing poles"},
  { "start": 75.0, "end": 79.0, "text": "curly tail up the hill," },
  { "start": 80.0, "end": 82.0, "text": "suddenly you're standing still" },
  { "start": 83.0, "end": 85.0, "text": "Your skin' so green" },
  { "start": 85.0, "end": 87.0, "text": "so beautiful," },
  { "start": 87.0, "end": 90.0, "text": "like an angel in disguise." },
  { "start": 91.0, "end": 92.0, "text": "But if you meet" },
  { "start": 93.0, "end": 95.0, "text": "a friendly penguin" },
  { "start": 95.0, "end": 97.0, "text": "will you communicate by" },
  { "start": 97.0, "end": 99.0, "text": "O-O-Open Source?" },
  { "start": 99.0, "end": 101.0, "text": "O-O-Open Source?" },
  { "start": 101.0, "end": 103.0, "text": "O-O-Open Source?" },
  { "start": 103.0, "end": 105.0, "text": "How will you talk with that?" },
  { "start": 105.0, "end": 107.0, "text": "Pe-e-e-enguin?" },
  { "start": 99.0, "end": 105.0, "text": "What's that chameleon say?" },
