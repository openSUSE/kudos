  { "start": 1.0, "end": 8.0, "text": "♪ Music plays ♪" },
  { "start": 8.0, "end": 15.0, "text": "Snakes go hiss, owls go hoot, horse goes snort and pigeons poop." },
  { "start": 16.0, "end": 22.0, "text": "Lions roar, sheep goes baaah," },
  { "start": 21.0, "end": 23.0, "text": "and the platypus says uh" },
